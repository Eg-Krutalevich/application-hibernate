<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entity.Car" table="cars">
        <id name="carId" type="car_id">
            <generator class="native"/>
        </id>
        <property name="name" type="string"/>
        <property name="year" type="integer"/>
        <property name="distance" type="integer"/>
        <property name="fuel" type="string"/>
        <property name="fuelConsumption" type="string"/>
        <property name="price" type="integer"/>
    </class>

</hibernate-mapping>
